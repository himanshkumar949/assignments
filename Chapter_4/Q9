#include <iostream>
using namespace std;
int main()
 {
 int userNum;
 cout << "Enter one of the numbers 1, 2, or 3: ";
 cin >> userNum; 
switch (userNum)
{
  case 1 : cout << "One";
  break; 
  case 2 : cout << "Two";
  break;
  case 3 : cout << "Three";
  break;
  default: cout << "Enter 1, 2, or 3 please.\n";
}
  return 0;
}
