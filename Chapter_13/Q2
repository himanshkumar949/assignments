#include <iostream>
using namespace std;
class Yard
{
private:
int length, width;
public:
Yard()
{ length = 0; width = 0; }
setLength(int len)
{ length = len; }
setWidth(int w)
{ width = w; }
};
int main()
{
Yard lawns[10];
cout << "Enter the length and width of each yard.\n";
for (int count = 0; count < 10; count++)
{
int input;
cout << "Yard " << (count + 1) << ":\n";
cout << "Length: ";
cin >> Input;
lawns[count].setLength(input);
cout << "width: ";
cin >> input;
lawns[count].setWidth(input);
}
return 0;
}

